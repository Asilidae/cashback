    "Yandex": {
      "pattern": "(.*?)(?=\\n\\d{2}\\.\\d{2}\\.\\d{4}\\nв \\d{2}:\\d{2})\\n\\d{2}\\.\\d{2}\\.\\d{4}\\nв \\d{2}:\\d{2}\\n(\\d{2}\\.\\d{2}\\.\\d{4})\\n(?:(\\*?\\d{4})\\n)?([+–]\\d+,\\d{2})",
      "format_datatime": "%d.%m.%Y",
      "param_groups": {"Дата операции": 1,
