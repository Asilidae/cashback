{
  "banks": {
    "GPB":{
      "pattern": "(\\d{2}\\.\\d{2}\\.\\d{4})\\n\\d{2}\\.\\d{2}\\.\\d{4}\\n(.*?)(?=[-+\\d ]+,\\d{2})(?:\\+0,00\\n)?([-+\\d ]+,\\d{2})",
      "format_datatime": "%d.%m.%Y",
      "param_groups": {"Дата операции": 0,
                       "Описание операции": 1,
                       "Сумма операции": 2},
      "head_cap": "Дата\\nсовершения\\nоперации\\nДата отражения\\nоперации по Счету\\nкарты\\n(списания/зачисления\\nденежных средств)\\nСодержание операции\\nПриход\\nРасход\\n",
      "column_description": 1,
      "column_type": 1,
      "column_cost": 2
    },

    "VTB":{
      "pattern": "(\\d{2}\\.\\d{2}\\.\\d{4})\\n\\d{2}:\\d{2}:\\d{2}\\n\\d{2}\\.\\d{2}\\.\\d{4}\\n[+-]?[\\d\\.]+ RUB\\n([+-]?[\\d\\.]+)\\n[\\d] RUB\\s+(.*?)(?=\\d{2}\\.\\d{2}\\.\\d{4}|Спасибо)",
      "format_datatime": "%d.%m.%Y",
      "param_groups": {"Дата операции": 0,
                       "Описание операции": 2,
                       "Сумма операции": 1},
      "head_cap": "Дата и время\\nоперации\\nДата обработки\\nбанком\\nСумма операции в\\nвалюте операции\\nСумма в валюте\\nкарты\\nКомиссия\\nОписание операции\\n",
      "column_description": 1,
      "column_type": 1,
      "column_cost": 2
    },

    "Ozon":{
      "pattern": "(\\d{2}\\.\\d{2}\\.\\d{4} \\d{2}:\\d{2}:\\d{2})\\n\\d+\\n([\\s\\S]*?)(?=\\n[-+] [\\d\\. ]+\\n)\\n([-+\\d\\. ]+)\\n",
      "format_datatime": "%d.%m.%Y %H:%M:%S",
      "param_groups": {"Дата операции": 0,
                       "Описание операции": 1,
                       "Сумма операции": 2},
      "head_cap": "Дата операции\\nДокумент\\nНазначение платежа\\nСумма операции\\n",
      "column_description": 1,
      "column_type": 1,
      "column_cost": 2
    },

    "Sber":{
      "column_description": 3,
      "column_type": 4,
      "column_cost": 5
    },

    "Pochta": {
      "pattern": "(\\d{2}\\.\\d{2}\\.\\d{4}) \\n\\d{2}:\\d{2}\\n\\d{2}\\.\\d{2}\\.\\d{4}\\n(?:\\*?\\d{4}\\n)?(.*?)(?=[-+\\d ]+[,]\\d{2})([-+\\d, ]+)",
      "format_datatime": "%d.%m.%Y",
      "param_groups": {"Дата операции": 0,
                       "Описание операции": 1,
                       "Сумма операции": 2},
      "head_cap": "Дата и \\nвремя \\nоперации\\nДата \\nсписания/ \\nзачисления\\nНомер \\nКарты\\nОписание операции\\nСумма в \\nвалюте \\nоперации\\nСумма в \\nвалюте \\nсчета\\n",
      "column_description": 1,
      "column_type": 1,
      "column_cost": 2
    },

    "AKBARS": {
      "pattern": "\\d{2}/\\d{2}/\\d{2}  \\d{2}:\\d{2}:\\d{2}\\n(\\d{2}/\\d{2}/\\d{2})\\n(.*?)(?=\\n\\d+\\n)\\n(.*?)(?=RUR)RUR\\n([\\d.]+)",
      "format_datatime": "%d/%m/%y",
      "param_groups": {"Дата операции": 0,
                       "Описание операции": 1,
                       "Сумма операции": 3},
      "head_cap": "Дата операции\\nДата \\nрасчета\\nСодержание операции\\nКод \\nавториза\\nции\\nСумма в валюте \\nоплаты\\nВалюта \\nоплаты\\nСумма в валюте \\nрасчетов\\nВалюта \\nрасчетов\\nСумма в \\nвалюте карты\\nКомиссия\\nИтого\\n",
      "column_description": 1,
      "column_type": 1,
      "column_cost": 2
    },

    "Alpha": {
      "pattern": "(\\d{2}\\.\\d{2}\\.\\d{4})\\n([A-Z0-9_]*?)\\n(.*?)(?=\\n[-0-9 ]*,[0-9]*)\\n([-0-9, ]*)",
      "format_datatime": "%d.%m.%Y",
      "param_groups": {"Дата операции": 0,
                       "Описание операции": 2,
                       "Сумма операции": 3},
      "head_cap": "Дата проводки\\nКод операции\\nОписание\\nСумма \\nв валюте счета\\n",
      "column_description": 1,
      "column_type": 1,
      "column_cost": 2
    },

    "MKB": {
      "pattern": "(\\d{2}\\.\\d{2}\\.\\d{4})\\n(.*?)(?=\\n[-0-9, ]* )\\n-?([-0-9, ]*)₽\\n([А-Яа-я]*)",
      "format_datatime": "%d.%m.%Y",
      "param_groups": {"Дата операции": 0,
                       "Описание операции": 1,
                       "Сумма операции": 2,
                       "Тип операции": 3},
      "head_cap": "Дата\\nОписание\\nСумма\\nТип\\nоперации\\nКод\\nавторизации\\n",
      "column_description": 1,
      "column_type": 3,
      "column_cost": 2
    },

    "Rayphayzen": {
      "pattern": "(\\d{2}\\.\\d{2}\\.\\d{4})\\n\\d{2}\\.\\d{2}\\.\\d{4}\\n\\d+\\n([–+]\\s[\\d\\xa0,]+) RUB\\n[–+]\\s[\\d\\xa0,]+ RUB\\n([\\s\\S]*?)(?=\\n\\d{2}\\.\\d{2}\\.\\d{4}|\\nВсего поступлений|\\nПродолжение)",
      "format_datatime": "%d.%m.%Y",
      "param_groups": {"Дата операции": 0,
                       "Описание операции": 2,
                       "Сумма операции": 1},
      "head_cap": "Дата операции\\nВыполнена банком\\nНомер\\nдокумента\\nСумма в валюте\\nопeрации\\nСумма в валюте\\nсчета\\nДетали операции\\nНомер\\nкарты\\n",
      "column_description": 1,
      "column_type": 1,
      "column_cost": 2
    },

    "Tbank": {
      "pattern": "(\\d{2}\\.\\d{2}\\.\\d{4})\\n\\d{2}:\\d{2}\\n\\d{2}\\.\\d{2}\\.\\d{4}\\n\\d{2}:\\d{2}\\n([+-]?[\\d\\s]+\\.\\d{2}).{2}\\n([+-]?[\\d\\s]+\\.\\d{2}).{2}\\n(.*?)(?=\\n\\d{4}|—)",
      "format_datatime": "%d.%m.%Y",
      "param_groups": {"Дата операции": 0,
                       "Описание операции": 3,
                       "Сумма операции": 2},
      "head_cap": "Дата и время\\nоперации\\nДата\\nсписания\\nСумма в валюте\\nоперации\\nСумма операции\\nв валюте карты\\nОписание\\nоперации\\nНомер\\nкарты\\n",
      "column_description": 1,
